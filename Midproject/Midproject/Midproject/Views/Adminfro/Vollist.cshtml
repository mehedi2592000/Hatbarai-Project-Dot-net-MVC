@model IEnumerable<Midproject.Models.En.VolunteerFrom>

@{
    ViewBag.Title = "Index";
}


<p>
    
    <form action="/Adminfro/vollist" method="get">
        <input type="text" name="search" placeholder="Enter the First name" />
        <input type="submit" name="submit" value="Search" />
    </form>

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Serial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nId_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Userserial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AgencyLogin.Agency_name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Serial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nId_number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Userserial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgencyLogin.Agency_name)
            </td>
            
        </tr>
    }

</table>

